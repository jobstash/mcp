OPENAI_API_KEY=

# JobStash MCP Server Configuration
# These are required for the @jobstash/mcp-server to start
JOBSTASH_SITE_URL=https://jobstash.xyz
JOBSTASH_API_URL=https://middleware.jobstash.xyz

# Optional: Name and version for the MCP server, defaults will be used if not set
# MCP_SERVER_NAME=jobstash-mcp-host
# MCP_SERVER_VERSION=0.1.0

# JobStash MCP Gateway Configuration
# OPENAI_MODEL defaults to gpt-4o in the NluService if not set
# OPENAI_MODEL=gpt-4o

# Required by @jobstash/file-parser (used in mcp-gateway) if using OpenAI for CV parsing
OPENAI_ASSISTANT_ID_PARSER=

# Port for the NL -> MCP Gateway (@jobstash/mcp-gateway)
# PORT=3000